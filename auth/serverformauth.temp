#!/bin/bash
SMAUTH=$1
SMAUTHSERVERBASE=$2
cat <<EOF
{
  "AllowAuthLevelOverride": false,
  "AllowSaveCreds": false,
  "AuthSchemeType": "HTMLForm",
  "IPCheck": false,
  "IgnorePwCheck": false,
  "IsRadius": false,
  "IsTemplate": false,
  "IsUsedbyAdmin": false,
  "Level": 5,
  "Library": "smauthhtml",
  "Name": "$SMAUTH",
  "Param": "$SMAUTHSERVERBASE/siteminderagent/forms/login.fcc;ACS=0;REL=0",
  "PersistSessionVars": false,
  "SmSAMLv1IdPs": [],
  "SmSAMLv2IdPs": [],
  "SmWSFEDIdPs": [],
  "SmWSSecurityIdPs": [],
  "SmXMLDCCIdPs": [],
  "SmXMLDSigIdPs": [],
  "SupportsValidateIdentity": false,
  "type": "SmAuthScheme"
}
EOF
